{
  "version": "0.0.1",
  "name": "yceffort-blog",
  "description": "yceffort-blog",
  "scripts": {
    "dev": "next dev",
    "sitemap": "node ./src/siteMap.js",
    "images": "node ./src/imageInfo.js",
    "build-nextjs": "next build",
    "build": "npm run sitemap && npm run images && npm run build-nextjs",
    "start": "next start",
    "lint": "eslint '**/*.{ts,tsx}'",
    "lint:fix": "npm run lint --fix",
    "prettier": "prettier '**/*.{json,md,yaml}' --check",
    "prettier:fix": "prettier '**/*.{json,md,yaml}' --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged -q"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "npm run prettier",
      "npm run lint",
      "git add"
    ]
  },
  "repository": "https://github.com/yceffort/yceffort-blog-v2",
  "author": "yceffort <yceffort@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "nextjs",
    "blog"
  ],
  "dependencies": {
    "@mapbox/rehype-prism": "^0.6.0",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/typography": "^0.4.0",
    "date-fns": "^2.21.3",
    "firebase-admin": "^9.8.0",
    "front-matter": "^4.0.2",
    "glob": "^7.1.7",
    "image-size": "^1.0.0",
    "memoizee": "^0.4.15",
    "next": "^10.2.0",
    "next-mdx-remote": "^3.0.1",
    "next-pwa": "^5.2.21",
    "next-seo": "^4.24.0",
    "next-themes": "0.0.14",
    "prismjs": "^1.23.0",
    "query-string": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "rehype-katex": "^5.0.0",
    "remark-math": "^4.0.0",
    "remark-prism": "^1.3.6",
    "remark-slug": "^6.0.0",
    "remark-toc": "^7.2.0",
    "tailwindcss": "^2.1.2",
    "unist-util-visit": "^3.1.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.5.0",
    "@tailwindcss/jit": "^0.1.18",
    "@types/date-fns": "^2.6.0",
    "@types/memoizee": "^0.4.5",
    "@types/node": "^15.0.3",
    "@types/react": "^17.0.5",
    "autoprefixer": "^10.2.5",
    "eslint-config-yceffort": "0.0.5",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "postcss": "^8.2.15",
    "typescript": "^4.2.4",
    "webpack": "^5.37.0"
  }
}
